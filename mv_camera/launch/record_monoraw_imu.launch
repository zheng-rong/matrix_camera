<launch>

<arg name="vehicle" default="hummer"/>
<arg name="filename" default="kalibr_bag"/>

<group ns="$(arg vehicle)">

	<node	pkg="rosbag"
			type="record"
			name="record"
			output="screen"
			args="-O /home/arkin/dataset_real/kalibr_bags/$(arg filename)
			/icc/imu
			/mono/image_raw"
			/>
			
</group>

</launch>
